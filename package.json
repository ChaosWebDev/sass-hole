{
  "name": "sass-hole",
  "version": "1.0.0",
  "description": "SASS/SCSS style/utility library. Very Lightweight!",
  "main": "dist/styles.css",
  "style": "src/index.scss",
  "scripts": {
    "build": "sass src/index.scss dist/styles.css",
    "dev": "sass --watch src/index.scss dist/styles.css"
  },
  "keywords": [
    "sass",
    "scss",
    "css",
    "utility",
    "flexbox",
    "grid",
    "spacing",
    "lightweight"
  ],
  "author": "Jordan Gerber",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.85.1"
  },
  "prepublishOnly": "npm run build",
  "files": [
    "dist/",
    "src/",
    "index.scss",
    "README.md"
  ],
  "exports": {
    ".": {
      "style": "./dist/styles.css",
      "sass": "./src/index.scss"
    }
  },
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChaosWebDev/sass-hole"
  },
  "bugs": {
    "url": "https://github.com/ChaosWebDev/sass-hole/issues"
  },
  "homepage": "https://github.com/ChaosWebDev/sass-hole#readme"
}
